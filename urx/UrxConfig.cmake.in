@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

set(HDF5_DIR "@HDF5_DIR@")
if(NOT "${HDF5_DIR}" STREQUAL "")
  find_dependency(
    HDF5
    NAMES
    hdf5
    COMPONENTS
    CXX
    @SEARCH_TYPE@
    CONFIG
    NO_DEFAULT_PATH
    REQUIRED)
endif()

set(WITH_PYTHON "@WITH_PYTHON@")
if(WITH_PYTHON)
  if(NOT WIN32)
    if(CMAKE_BUILD_TYPE STREQUAL "Debug")
      set(Python3_FIND_ABI "ON" "ANY" "ANY")
    else()
      set(Python3_FIND_ABI "OFF" "ANY" "ANY")
    endif()
  endif()
  find_dependency(
    Python3
    COMPONENTS
    Interpreter
    Development
    REQUIRED)
  find_dependency(
    pybind11
    CONFIG
    REQUIRED)
endif()

set(URX_PYTHON_TEST_PATH "@CMAKE_INSTALL_PREFIX@/@CMAKE_INSTALL_DATAROOTDIR@/@INSTALL_NAME@/python")
set(MATLAB_INSTALL_DATADIR "@CMAKE_INSTALL_PREFIX@/@MATLAB_INSTALL_DATADIR@")

include("${CMAKE_CURRENT_LIST_DIR}/UrxTargets.cmake")

check_required_components(Urx UrxUtils)
